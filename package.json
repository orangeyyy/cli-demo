{
  "name": "cli-demo",
  "version": "1.0.0",
  "description": "命令行工具demo",
  "main": "index.js",
  "bin": {
    "bana": "./bin/index.js"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --ignore 'src/**/templates/' --copy-files --include-dotfiles --extensions '.ts'",
    "dev": "babel src -d lib --watch --skip-initial-build --extensions '.ts'",
    "prettier": "prettier --write \"./src/**/*.{ts,ts,json}\""
  },
  "keywords": [
    "cli"
  ],
  "author": "orangeyyy",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/parser": "^5.10.1",
    "commander": "^8.3.0",
    "core-js": "^3.20.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/commander": "^2.12.2",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "eslint": "^8.7.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
