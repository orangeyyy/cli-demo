{
  "name": "<%= name%>",
  "react": "<%= react%>",
  "type": "<%= type%>",
  "version": "1.0.0",
  "description": "命令行工具demo",
  "main": "index.js",
  "bin": {
    "bana": "./bin/index.js"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --ignore 'src/**/templates/' --copy-files --include-dotfiles --extensions '.ts'",
    "dev": "babel src -d lib --watch --skip-initial-build --extensions '.ts'",
    "prettier": "prettier --write \"./src/**/*.{ts,js,json}\""
  },
  "keywords": ["<%- tags.join('","')%>"],
  "escape": "<%= escapeStr%>",
  "noEscape": "<%- escapeStr%>",
  "noSpace": "<%_ escapeStr_%>",
  "author": "orangeyyy",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}
